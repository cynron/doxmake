#!/usr/bin/env node
/**
 * Module dependencies
 */
var program = require('commander');
var doxTemplate = require('../');


program.version(doxTemplate.version);

program.on('--help', function() {

		console.log(' Examples:');
		console.log('');
		console.log('  $doxTemplate [name] <folder> ');
		console.log('');
});

program.parse(process.argv);


